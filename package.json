{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "homepage": "loan-application",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "heroku": "^7.65.0",
    "playwright": "^1.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-icons": "^4.6.0",
    "react-phone-number-input": "^3.2.11",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "reactjs-popup": "^2.0.5",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "test:e2e": "playwright test  .spec.js --workers=20%  ",
    "build": "set \"CI=false\" && react-scripts build && cd build && jar -cvf web.war .",
    "test": "react-scripts test --runTestsByPath --env=jsdom --watchAll=false --testMatch **/src/**/*.test.js --coverage",
    "eject": "react-scripts eject",
    "compile": "npm run build && cd build && jar -cvf web.war .",
    "login":"heroku login"
  },
  "eslintConfig": {},
  "engines": {
    "node": "18.x",
    "npm":"8.x"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/redux/",
      "<rootDir>/src/reportWebVitals.js",
      "<rootDir>/src/index.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1"
  }
}
